{
  "expo": {
    "name": "Parkly",
    "slug": "Parkly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/parkly.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "parkly",                 
    "platforms": ["ios", "android", "web"], 
    "splash": {
      "image": "./assets/parkly.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/parkly.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/parkly.png"
    },
    "plugins": [
      "expo-web-browser"
    ]
  }
}
